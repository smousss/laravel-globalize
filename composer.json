{
    "name": "smousss/laravel-globalize",
    "description": "Internationalize Laravel projects in a matter of seconds!",
    "keywords": [
        "i18n",
        "internationalization",
        "laravel",
        "laravel-globalize",
        "localization",
        "smousss",
        "translation"
    ],
    "homepage": "https://github.com/smousss/laravel-globalize",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Crozat",
            "email": "hello@benjamincrozat.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Smousss\\Laravel\\Globalize\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smousss\\Laravel\\Globalize\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smousss\\Laravel\\Globalize\\GlobalizeServiceProvider"
            ],
            "aliases": {
                "Globalize": "Smousss\\Laravel\\Globalize\\Facades\\Globalize"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
